<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="433.0" prefWidth="551.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.NaujoKatalogoForma">
    <children>
        <Button fx:id="backButton" layoutX="51.0" layoutY="376.0" mnemonicParsing="false" onAction="#backToCatalogueWindow" text="Back to main menu" />
        <Button fx:id="createCatalogue" layoutX="45.0" layoutY="336.0" mnemonicParsing="false" onAction="#addNewCatalogue" text="Create new catalogue" />
        <TextField fx:id="nameC" layoutX="36.0" layoutY="132.0" prefHeight="25.0" prefWidth="149.0" promptText="Catalogue name" />
        <TextField fx:id="rootC" layoutX="35.0" layoutY="249.0" promptText="Root catalogue name" />
        <DatePicker fx:id="dateCreatedC" layoutX="35.0" layoutY="205.0" prefHeight="25.0" prefWidth="149.0" promptText="Date created" />
        <TextField fx:id="parentC" layoutX="36.0" layoutY="292.0" prefHeight="25.0" prefWidth="149.0" promptText="Parent catalogue name" />
      <TextField fx:id="descC" layoutX="35.0" layoutY="169.0" promptText="Catalogue description" />
      <Label layoutX="14.0" layoutY="44.0" prefHeight="27.0" prefWidth="509.0" text="To create new root catalogue, type root and parent name as {--}">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Label layoutX="16.0" layoutY="87.0" prefHeight="27.0" prefWidth="483.0" text="To create new parent catalogue, type root name as {--}">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
    </children>
</AnchorPane>
